<!--Header Start-->
<div class="content-wrapper">
    <header class="header">
        <h1 class="main-title">
            Next Generation Job Platform Designed To Simplify<br />
            <span class="highlight">Hiring & Job Searching</span>
        </h1>
    </header>

    <div class="main-content">
        <div class="image-section">
            <div class="image-card">
                <img src="https://encrypted-tbn0.gstatic.com/images?q=tbn:ANd9GcRpLty99Wssr7YdtYsl3mhhowq4XbIfvjcolHpaXE9ptHaefcm-06MMgqI&s" />
            </div>
        </div>

        <div class="features-section">
            <div class="feature">
                <div class="feature-icon icon-recommendations">
                    <div class="paper-icon">
                        <div class="paper-lines"></div>
                        <div class="checkmark">✓</div>
                    </div>
                </div>
                <div class="feature-text">
                    <h3>Instant recommendations<br> based on your skills</h3>
                </div>
            </div>

            <div class="feature">
                <div class="feature-icon icon-manage">
                    <div class="manage-icon">
                        <div class="document-mini"></div>
                        <div class="person-circle"></div>
                    </div>
                </div>
                <div class="feature-text">
                    <h3>Post jobs, manage applicants,<br>and hire faster</h3>
                </div>
            </div>

            <div class="feature">
                <div class="feature-icon icon-apply">
                    <div class="apply-icon">
                        <div class="browser-window">
                            <div class="browser-dots"></div>
                            <div class="browser-content"></div>
                        </div>
                        <div class="click-hand">▶</div>
                    </div>
                </div>
                <div class="feature-text">
                    <h3>Apply to jobs in just <br>one click</h3>
                </div>
            </div>
        </div>
    </div>
    <!--Header End-->
    <!--Job Container Start-->

    <div class="job-container">
        <div class="job-grid">
            <!-- Left Side -->
            <div class="job-list">
                <div class="job-card">
                    <div class="job-title">Frontend Developer</div>
                    <div class="job-count">10K+ Jobs</div>
                </div>
                <div class="job-card">
                    <div class="job-title">UX/UI Designer</div>
                    <div class="job-count">30K+ Jobs</div>
                </div>
                <div class="job-card">
                    <div class="job-title">Backend Developer</div>
                    <div class="job-count">20K+ Jobs</div>
                </div>
                <div class="job-card">
                    <div class="job-title">Dot Net Developer</div>
                    <div class="job-count">15K+ Jobs</div>
                </div>
            </div>

            <!-- Center -->
            <div class="center-box">
                <h2>Trending <span>Jobs</span> with<br> high demand</h2>
                <img src="https://encrypted-tbn0.gstatic.com/images?q=tbn:ANd9GcTTJCJPgCNvd9YhzhbzOr17Hr9kZPAwauUX2A&s" alt="Job Character" />
                <button class="apply-btn">Apply For A Job</button>
            </div>

            <!-- Right Side -->
            <div class="job-list">
                <div class="job-card">
                    <div class="job-title">Graphic Designer</div>
                    <div class="job-count">10K+ Jobs</div>
                </div>
                <div class="job-card">
                    <div class="job-title">Full Stack Developer</div>
                    <div class="job-count">30K+ Jobs</div>
                </div>
                <div class="job-card">
                    <div class="job-title">Interaction Designer</div>
                    <div class="job-count">20K+ Jobs</div>
                </div>
                <div class="job-card">
                    <div class="job-title">Java Developer</div>
                    <div class="job-count">15K+ Jobs</div>
                </div>
            </div>
        </div>
    </div>


    <!--Job Container End-->
    <!--Hero Container Start-->
    <section class="hero container">
        <div class="hero-left">
            <h1 class="headline">Be A <span class="accent">Freelancer</span>, Be Independent</h1>
            <p class="sub">Work with clients worldwide, build your portfolio, and get paid for your skills</p>
        </div>
        <div class="hero-right">
            <button class="btn-outline">Explore Freelancing</button>
        </div>
    </section>
    <!--Hero Container End-->
    <!-- Feature cards Start-->
